    "eslint-plugin-storybook": "^10.0.0",
    "eslint-plugin-unicorn": ">=58.0.0",
    "typescript-eslint": "^8.15.0"
  },
  "devDependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@eslint/js": "^9.39.2",
    "@types/confusing-browser-globals": "^1.0.3",
