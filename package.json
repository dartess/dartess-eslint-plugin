{
  "name": "@dartess/eslint-plugin",
  "type": "module",
  "version": "0.0.1",
  "description": "A set of rules and configs for various react projects",
  "keywords": [
    "eslint",
    "eslint-plugin"
  ],
  "author": "Sergey Kozlov",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "ISC",
  "exports": {
    ".": "./dist/index.js",
    "./recommended": "./dist/configs/recommended.js",
    "./react": "./dist/configs/react.js",
    "./next": "./dist/configs/next.js",
    "./mobx": "./dist/configs/mobx.js",
    "./storybook": "./dist/configs/storybook.js",
    "./utils": "./dist/utils/index.js"
  },
  "scripts": {
    "lint": "eslint .",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "test": "node --import ./test-setup.ts --test --experimental-strip-types",
    "ts": "tsc",
    "prepare": "husky"
  },
  "engines": {
    "node": ">= 20.0.0"
  },
  "files": [
    "dist",
    "docs",
    "CHANGELOG.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dartess/dartess-eslint-plugin"
  },
  "peerDependencies": {
